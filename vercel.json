{
    "version": 2,
    "builds": [
      {
        "src": "DriveDeal/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      },
      {
        "src": "DriveDeal/Server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "DriveDeal/Server/$1"
      },
      {
        "src": "/(.*)",
        "dest": "DriveDeal/dist/$1"
      }
    ]
  }
  